{
  "name": "subtitles-generator",
  "version": "1.0.0",
  "description": "A subtitle generator application using Gemini AI",
  "homepage": "./",
  "scripts": {
    "start": "cross-env PORT=3008 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "server:start": "node server.js",
    "python:start": "uv run server/narrationApp.py",
    "python:test": "uv run server/test-narration-service.py",
    "video-renderer:build": "cd video-renderer && npm run server:build",
    "video-renderer:start": "cd video-renderer && npm run server:start",
    "dev": "concurrently \"npm run start\" \"npm run server:start\" \"npm run video-renderer:start\"",
    "dev:test": "concurrently \"npm run start\" \"npm run server:start\" \"npm run python:test\"",
    "check:uv": "node check-uv.js",
    "test:python": "uv run test-python.py",
    "create:venv": "uv venv .venv",
    "install:f5tts": "uv pip install -e F5-TTS",
    "install:deps": "uv pip install flask flask-cors soundfile numpy torch torchaudio vocos",
    "install:yt-dlp": "node install-yt-dlp.js",
    "install:all": "npm install && node setup-narration.js && node install-yt-dlp.js",
    "postinstall": "node install-yt-dlp.js && npm run video-renderer:build",
    "setup:f5tts": "node setup-f5tts.js",
    "python:start:cuda": "uv run -- python server/narrationApp.py",
    "dev:cuda": "cross-env START_PYTHON_SERVER=true concurrently \"npm run start\" \"npm run server:start\"",
    "install:all:uv": "npm install && node setup-narration-uv.js && node install-yt-dlp.js",
    "python:start:uv": "uv run -- python server/narrationApp.py",
    "dev:uv": "concurrently \"npm run start\" \"npm run server:start\" \"npm run python:start:uv\"",
    "setup:narration:uv": "node setup-narration.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/genai": "^0.9.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@react-oauth/google": "^0.12.1",
    "@remotion/player": "^4.0.314",
    "adm-zip": "^0.5.16",
    "assert": "^2.1.0",
    "axios": "^1.8.4",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "genius-lyrics": "^4.4.7",
    "googleapis": "^148.0.0",
    "http-proxy-middleware": "^3.0.5",
    "https-browserify": "^1.0.0",
    "i18next": "^23.8.0",
    "multer": "^1.4.5-lts.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "puppeteer": "^24.7.2",
    "puppeteer-core": "^24.7.2",
    "querystring-es3": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.11",
    "remotion": "^4.0.314",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "web-vitals": "^3.5.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
